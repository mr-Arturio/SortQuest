# Client-side Firebase (NEXT_PUBLIC_* are exposed to the browser)
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=000000000000
NEXT_PUBLIC_FIREBASE_APP_ID=1:000000000000:web:abcdef123456

# AWS Bedrock (server-side)
AWS_REGION=us-east-1
BEDROCK_VISION_MODEL_ID=amazon.nova-lite-v1:0

AWS_BEARER_TOKEN_BEDROCK = your_token

# JWT signing for session tokens (server-side)
SESSION_SECRET=replace_with_a_secure_random_string

# Demo/Test feature flags
NEXT_PUBLIC_TEST_MODE=0
NEXT_PUBLIC_DEMO_NO_QR=0


